<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="test-suite" verbose="11" parallel="tests" thread-count="3">

	<test name="Google test">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url" value="https://www.google.co.in/"></parameter>
	
		<classes>
			<class name="testngconcept.GoogleTest"></class>
		</classes>
	</test>
	
	<test name="Facebook test">
	<parameter name="browser" value="firefox"></parameter>
	<parameter name="url" value="https://www.facebook.com/login/"></parameter>
	
		<classes>
			<class name="testngconcept.FacebookTest"></class>
		</classes>
	</test>
	
	<test name="Opencart test">
	<parameter name="browser" value="edge"></parameter>
	<parameter name="url" value="https://naveenautomationlabs.com/opencart/"></parameter>

		<classes>
			<class name="testngconcept.OpenCartTest"></class>
		</classes>
	</test>


</suite>